{
	"name": "@zeal-ui/core",
	"version": "2.0.7",
	"description": "Zeal UI is a simple, minimalistic component library to build your awesome apps faster.",
	"browser": "dist/web.js",
	"main": "dist/node.js",
	"files": [
		"dist"
	],
	"scripts": {
		"start": "watch \"npm run build && yalc publish --push\" ./src",
		"build": "webpack"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"styled-components": "^5.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.16.10",
		"@babel/plugin-syntax-jsx": "^7.16.7",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"babel-loader": "^8.2.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-styled-components": "^2.0.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-runtime": "^6.26.0",
		"css-loader": "^6.5.1",
		"watch": "^1.0.2",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1"
	},
	"babel": {
		"presets": [
			"@babel/preset-react",
			"@babel/preset-env"
		],
		"plugins": [
			"babel-plugin-styled-components",
			"@babel/plugin-syntax-jsx"
		]
	},
	"author": {
		"name": "M V Ganesh Kumar"
